<?xml version="1.0" encoding="UTF-8"?>

<Defs>

  <!-- Colonial Beret -->

  <ThingDef ParentName="HatMakeableBase">
    <defName>pphhyy_ColonialBeret</defName>
    <label>colonial beret</label>
    <description>The comfort wear of the corps, is what the officers would tell you. When not murdering bugs or other less then friendly aliens the Marines wear these during down time.</description>
    <possessionCount>1</possessionCount>
    <recipeMaker>
      <researchPrerequisite>pphhyy_URMCBasic</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>HelmetsAndHats/ColonialBeret/ColonialBeret</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>1.5</drawSize>
    </graphicData>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToMake>1800</WorkToMake>
      <Mass>0.14</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.10</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.50</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <developmentalStageFilter>Child, Adult</developmentalStageFilter>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>HelmetsAndHats/ColonialBeret/ColonialBeret</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>URMC_ColonialBeret</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
      <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
     <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>(95, 99, 76, 255)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(95, 99, 76, 255)</only>
        </li>
        <li>
          <weight>10</weight>
          <only>(95, 99, 76, 255)</only>
        </li>
        <li>
          <weight>6</weight>
          <min>(95, 99, 76, 255)</min>
          <max>(95, 99, 76, 255)</max>
        </li>
      </options>
    </colorGenerator>
    <tradeTags>
      <li>BasicClothing</li>
    </tradeTags>
  </ThingDef>

